<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_4_1.xsd"
	version="4.1">

	<servlet>
		<servlet-name>TServletExample</servlet-name>
		<servlet-class>com.share.soa.thrift.http.TServletExample</servlet-class>
		<async-supported>true</async-supported>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>TServletExample</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>

</web-app>